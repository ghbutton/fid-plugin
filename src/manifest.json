{
  "manifest_version": 2,
  "name": "FIdentity",
  "version": "1.0",
  "description": "Federated Identity",
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
  "browser_action": {
    "default_title": "Federated Identity",
    "default_popup": "popup/index.html"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": [
    "storage"
  ]
}

